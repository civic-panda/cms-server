{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [1],
    "no-underscore-dangle": 0,
    "strict": 0,
    "no-console": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "arrow-body-style": [0],
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0
  }
}
